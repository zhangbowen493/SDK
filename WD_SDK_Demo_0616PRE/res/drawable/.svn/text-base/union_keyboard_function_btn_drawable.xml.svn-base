<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/union_keyboard_function_btn_shadow_color" />
            <stroke android:width="1dp" android:color="@color/union_keyboard_function_btn_shadow_color" />
            <corners android:radius="2dp"/>
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="@color/union_keyboard_function_btn_background_color" />
            <stroke android:width="1dp" android:color="@color/union_keyboard_function_btn_background_color" />
            <corners android:radius="2dp"/>
        </shape>
    </item>

</selector>
